{
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": ["js", "json", "jsx", "node", "ts", "tsx"],
    "transform": {
        "^.+\\.tsx?$": [
            "ts-jest",
            {
                "tsconfig": {
                    "allowJs": true
                }
            }
        ]
    },
    "preset": "ts-jest"
}
